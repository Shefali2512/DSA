char findTheDifference(char *s, char *t) {
    char result = 0;

    // XOR all characters in s
    while (*s) {
        result ^= *s;
        s++;
    }

    // XOR all characters in t
    while (*t) {
        result ^= *t;
        t++;
    }

    return result; // The extra character remains after XORing
}
